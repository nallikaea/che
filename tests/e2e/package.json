  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint --fix -p .",
    "tsc": "rm -rf ./dist && tsc -p .",
    "test": "npm run lint && npm run tsc && export USERSTORY=$USERSTORY && mocha --config dist/configs/mocharc.js",
    "driver-less-test": "export TS_USE_WEB_DRIVER_FOR_TEST=false && npm run test",
    "delayed-test": "npm run lint && npm run tsc && export TS_USE_WEB_DRIVER_FOR_TEST=false && mocha --config dist/configs/mocharc.js --delay",
    "regression-suite": "configs/sh-scripts/full-regresion-run-x86_64.sh",
    "report-demo": "allure generate .allure-results-demo --clean -o .allure-report && allure open .allure-report",
    "report": "allure generate .allure-results --clean -o .allure-report && allure open .allure-report"
  },
  "author": "Ihor Okhrimenko (iokhrime@redhat.com)",
  "license": "ISC",
